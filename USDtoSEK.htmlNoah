    <title>Document</title>
</head>
<body>
    <input type="number" id="SEK">
    <button id="SEKtoNOK" onclick="SEKtoNOK()">SEK til NOK</button>
    <span id ="NOK"></span>

    <script>
        async function SEKtoNOK() {
            var svenskKroner = document.getElementById("SEK").value;
            var svensk_kurs = await hentKurs(SEK);
            var amerikanske_dollar = svenskKroner/svensk_kurs;
            var norskKurs = await hentNOK();
            document.getElementById("NOK").innerHTML = norske_kroner;
        }

        async function hentSEK(valutaForkortelse){
            var forespørsel = await fetch("https://openexchangerates.org/api/latest.json?app_id=062ea16b4bed4ee9946a29df8ec67aaa")
            var svar = await forespørsel.json()
            var valuta_kurs = svar.rates.SEK
            return valuta_kurs
        }  

        async function hentNOK(valutaForkortelse){
            var forespørsel = await fetch("https://openexchangerates.org/api/latest.json?app_id=062ea16b4bed4ee9946a29df8ec67aaa")
            var svar = await forespørsel.json()
            var valuta_kurs = svar.rates.NOK
            return valuta_kurs
        }  
    </script>
</body>
